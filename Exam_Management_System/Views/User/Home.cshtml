
@{
    ViewData["Title"] = "Home";
    Layout = null;
}

<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
<link rel="stylesheet" href="~/css/site.css" />
<link rel="stylesheet" href="~/css/bootstrap-timepicker.css" />
<link rel="stylesheet" href="~/css/bootstrap-datepicker.css" />

<link rel="stylesheet" href="~/css/adminLTE/font-awesome/css/font-awesome.css" />
<link rel="stylesheet"
      href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
<div class="container " style="margin-top:30px">
    <div class="row ">
        <div class="col-md-4 col-md-offset-4">
            <div class="panel panel-info">
                <div class="panel-heading ">
                    <center>
                        <h1>Registration Form</h1>
                    </center>
                </div>
                <div class="panel-body text-center" style="margin-top:30px;margin-bottom:30px;">
                    <a class="btn btn-primary btn-lg" asp-controller="User" asp-action="Index" style="margin-bottom:10px">New Student Form</a><br />
                    <a class="btn btn-success btn-lg" asp-controller="User" asp-action="OldForm" style="margin-bottom:10px">Old Student Form</a><br />
                    <a class="btn" asp-controller="Account" asp-action="Index">admin</a>
                </div>
                <div class="panel-footer text-center">
                    <a class="underlineHover" href="http://ucsthn.moe.edu.mm">Go to UCS Thaton Site</a>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/lib/jquery/dist/jquery.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>

<script src="~/js/bootstrap-timepicker.js"></script>
<script src="~/js/bootstrap-datepicker.js"></script>